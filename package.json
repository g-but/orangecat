{
  "name": "orangecat",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "d": "./scripts/dev-start.js"
  },
  "scripts": {
    "d": "node scripts/dev-start.js",
    "dev": "next dev",
    "dev:clean": "bash scripts/dev.sh",
    "dev:win": "scripts/dev.bat",
    "kill:node": "powershell \"Stop-Process -Name node -Force -ErrorAction SilentlyContinue\"",
    "clear:cache": "rm -rf .next && npm cache clean --force",
    "fresh:start": "npm run kill:node && npm run clear:cache && npm run dev",
    "dev:port": "next dev -p 3020",
    "dev:full": "concurrently \"npm run dev\" \"supabase start\" --names \"NEXT,SUPABASE\" --prefix-colors \"cyan,green\"",
    "kill-ports": "powershell \"Get-Process node -ErrorAction SilentlyContinue | Stop-Process -Force\"",
    "build": "next build",
    "build:production": "NODE_ENV=production next build && npm run analyze",
    "build:analyze": "ANALYZE=true npm run build",
    "analyze": "node scripts/analyze-bundle.js",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:security": "jest --testPathPattern=security",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:auth": "jest tests/auth --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:brave": "playwright test --project=brave",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:e2e",
    "deploy:button": "node scripts/one-button-deploy.js",
    "deploy:staging": "node scripts/one-button-deploy.js staging",
    "deploy:production": "node scripts/one-button-deploy.js production",
    "deploy:force": "node scripts/one-button-deploy.js production --force",
    "deploy:quick": "vercel --prod",
    "deploy:rollback": "node scripts/rollback.js",
    "env:pull": "vercel env pull .env.local",
    "env:push": "node scripts/sync-env.js",
    "env:validate": "node scripts/validate-env.js",
    "predeploy": "npm run env:push && npm run test:security",
    "postdeploy": "npm run test:e2e:production",
    "postinstall": "npm install --include=optional sharp || echo 'Sharp optional install failed'",
    "precommit": "npm run lint:check && npm run type-check && npm run test:unit",
    "prepare": "husky install || echo 'Husky not installed'",
    "health:check": "curl -f $PRODUCTION_URL/api/health || echo 'Health check failed'",
    "monitor:logs": "vercel logs --follow",
    "monitor:metrics": "node scripts/monitor-metrics.js",
    "optimize:images": "node scripts/optimize-images.js",
    "security:scan": "npm audit && npm run test:security",
    "performance:check": "node scripts/performance-check.js",
    "deployment:verify": "node scripts/verify-deployment-setup.js",
    "deployment:setup": "node scripts/verify-deployment-setup.js",
    "w": "./w",
    "deploy": "./w",
    "deploy:monitor": "node scripts/deployment-monitor.js",
    "mcp:server": "node mcp-server.js",
    "mcp:start": "concurrently \"npm run dev\" \"node scripts/mcp-server.js\" --names \"NEXT,MCP\" --prefix-colors \"cyan,magenta\"",
    "mcp:test": "concurrently \"npm run mcp:server\" \"npm run test:e2e:ui\"",
    "browser": "node scripts/browser-automation.js",
    "format": "prettier --write .",
    "docs:check-links": "node scripts/check-documentation-links.js",
    "docs:check-links:ci": "node scripts/check-documentation-links.js --ci",
    "bundle:analyze": "ANALYZE=true npm run build",
    "bundle:monitor": "node scripts/bundle-size-monitor.js",
    "bundle:check": "npm run build && npm run bundle:monitor"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@modelcontextprotocol/server-github": "^2025.4.8",
    "@next/mdx": "^15.3.3",
    "@octokit/rest": "^22.0.0",
    "@playwright/mcp": "^0.0.29",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-progress": "^1.1.6",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.2",
    "@types/lodash": "^4.17.20",
    "@types/mdx": "^2.0.13",
    "@vercel/analytics": "^1.5.0",
    "@vercel/mcp-adapter": "^0.11.2",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.0.1",
    "bitcoinjs-lib": "^6.1.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.20.1",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "lucide-react": "^0.488.0",
    "next": "^15.3.3",
    "next-mdx-remote": "^5.0.0",
    "postcss": "^8.5.6",
    "qrcode": "^1.5.3",
    "qrcode.react": "^4.2.0",
    "react": "^18",
    "react-dom": "^18",
    "rehype-highlight": "^7.0.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.76",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "chalk": "^5.4.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^8",
    "eslint-config-next": "^15.3.3",
    "eslint-plugin-jsdoc": "^48.11.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.53.0",
    "react-router-dom": "^7.6.3",
    "ts-jest": "^29.1.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "ws": "^8.18.2"
  },
  "optionalDependencies": {
    "sharp": "^0.34.2"
  }
}
