        (cov_3octjqh1y().b[9][1]++, numAmount.toLocaleString('en-US'));
      case 'CHF':
        /* istanbul ignore next */
        cov_3octjqh1y().b[6][3]++;
        cov_3octjqh1y().s[17]++;
        return showSymbol ?
        /* istanbul ignore next */
        (cov_3octjqh1y().b[10][0]++, `${numAmount.toLocaleString('en-US')} CHF`) :
        /* istanbul ignore next */
        (cov_3octjqh1y().b[10][1]++, numAmount.toLocaleString('en-US'));
      default:
        /* istanbul ignore next */
        cov_3octjqh1y().b[6][4]++;
        cov_3octjqh1y().s[18]++;
        return showSymbol ?
        /* istanbul ignore next */
        (cov_3octjqh1y().b[11][0]++, `${numAmount} ${currency}`) :
        /* istanbul ignore next */
        (cov_3octjqh1y().b[11][1]++, numAmount.toString());
    }
  };
  /* istanbul ignore next */
  cov_3octjqh1y().s[19]++;
  return (0, jsx_runtime_1.jsx)("span", {
    className: (0, utils_1.cn)(colorConfig.className, sizeClasses[size], 'font-mono tabular-nums', className),
    style: colorConfig.style,
    children: formatAmount(amount, currency)
  });
};
/* istanbul ignore next */
cov_3octjqh1y().s[20]++;
exports.CurrencyDisplay = CurrencyDisplay;
