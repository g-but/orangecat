      cov_2ne6nouzzk().s[54]++;
      switch (selectedTimeFilter) {
        case 'thisyear':
          /* istanbul ignore next */
          cov_2ne6nouzzk().b[11][0]++;
          cov_2ne6nouzzk().s[55]++;
          filtered = filtered.filter(post => {
            /* istanbul ignore next */
            cov_2ne6nouzzk().f[15]++;
            cov_2ne6nouzzk().s[56]++;
            return new Date(post.date).getFullYear() === now.getFullYear();
          });
          /* istanbul ignore next */
          cov_2ne6nouzzk().s[57]++;
          break;
        case 'last6months':
          /* istanbul ignore next */
          cov_2ne6nouzzk().b[11][1]++;
          const sixMonthsAgo =
          /* istanbul ignore next */
          (cov_2ne6nouzzk().s[58]++, new Date());
          /* istanbul ignore next */
          cov_2ne6nouzzk().s[59]++;
          sixMonthsAgo.setMonth(sixMonthsAgo.getMonth() - 6);
          /* istanbul ignore next */
          cov_2ne6nouzzk().s[60]++;
          filtered = filtered.filter(post => {
            /* istanbul ignore next */
            cov_2ne6nouzzk().f[16]++;
            cov_2ne6nouzzk().s[61]++;
            return new Date(post.date) >= sixMonthsAgo;
          });
          /* istanbul ignore next */
          cov_2ne6nouzzk().s[62]++;
          break;
        case 'thismonth':
          /* istanbul ignore next */
          cov_2ne6nouzzk().b[11][2]++;
          cov_2ne6nouzzk().s[63]++;
          filtered = filtered.filter(post => {
            /* istanbul ignore next */
            cov_2ne6nouzzk().f[17]++;
            const postDate =
            /* istanbul ignore next */
            (cov_2ne6nouzzk().s[64]++, new Date(post.date));
            /* istanbul ignore next */
            cov_2ne6nouzzk().s[65]++;
            return /* istanbul ignore next */(cov_2ne6nouzzk().b[12][0]++, postDate.getFullYear() === now.getFullYear()) &&
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[12][1]++, postDate.getMonth() === now.getMonth());
          });
          /* istanbul ignore next */
          cov_2ne6nouzzk().s[66]++;
          break;
        default:
          /* istanbul ignore next */
          cov_2ne6nouzzk().b[11][3]++;
          cov_2ne6nouzzk().s[67]++;
          // Handle specific year-month (YYYY-MM format)
          if (selectedTimeFilter.includes('-')) {
            /* istanbul ignore next */
            cov_2ne6nouzzk().b[13][0]++;
            const [year, month] =
            /* istanbul ignore next */
            (cov_2ne6nouzzk().s[68]++, selectedTimeFilter.split('-'));
            /* istanbul ignore next */
            cov_2ne6nouzzk().s[69]++;
            filtered = filtered.filter(post => {
              /* istanbul ignore next */
              cov_2ne6nouzzk().f[18]++;
              const postDate =
              /* istanbul ignore next */
              (cov_2ne6nouzzk().s[70]++, new Date(post.date));
              /* istanbul ignore next */
              cov_2ne6nouzzk().s[71]++;
              return /* istanbul ignore next */(cov_2ne6nouzzk().b[14][0]++, postDate.getFullYear() === parseInt(year)) &&
              /* istanbul ignore next */
              (cov_2ne6nouzzk().b[14][1]++, postDate.getMonth() === parseInt(month) - 1);
            });
          } else
          /* istanbul ignore next */
          {
            cov_2ne6nouzzk().b[13][1]++;
          }
      }
    } else
    /* istanbul ignore next */
    {
      cov_2ne6nouzzk().b[10][1]++;
    }
    cov_2ne6nouzzk().s[72]++;
    return filtered;
  }, [posts, selectedTag, selectedTimeFilter]));
  // When no tag filter and All Time selected, show featured post separately + all other posts
  // When any filter is selected, show all matching posts in grid (no separate featured section)
  const postsToShow =
  /* istanbul ignore next */
  (cov_2ne6nouzzk().s[73]++,
  /* istanbul ignore next */
  (cov_2ne6nouzzk().b[16][0]++, selectedTag) ||
  /* istanbul ignore next */
  (cov_2ne6nouzzk().b[16][1]++, selectedTimeFilter !== 'all') ?
  /* istanbul ignore next */
  (cov_2ne6nouzzk().b[15][0]++, filteredPosts // Show all filtered posts including featured
  ) :
  /* istanbul ignore next */
  (cov_2ne6nouzzk().b[15][1]++, posts)); // Show ALL posts in grid when "All Topics" is selected
  // Only show featured section when no filters AND user hasn't selected any specific filtering
  const showFeaturedSection =
  /* istanbul ignore next */
  (cov_2ne6nouzzk().s[74]++, false); // Disable featured section for now to test unified grid
  /* istanbul ignore next */
  cov_2ne6nouzzk().s[75]++;
  return (0, jsx_runtime_1.jsxs)("div", {
    className: "max-w-6xl mx-auto",
    children: [
    /* istanbul ignore next */
    (cov_2ne6nouzzk().b[17][0]++, showFeaturedSection) &&
    /* istanbul ignore next */
    (cov_2ne6nouzzk().b[17][1]++, featuredPost) &&
    /* istanbul ignore next */
    (cov_2ne6nouzzk().b[17][2]++, (0, jsx_runtime_1.jsx)("div", {
      className: "mb-12",
      children: (0, jsx_runtime_1.jsxs)("div", {
        className: "bg-gradient-to-r from-orange-50 to-tiffany-50 rounded-2xl p-8 border border-orange-100",
        children: [(0, jsx_runtime_1.jsxs)("div", {
          className: "flex items-center text-sm text-orange-600 mb-4 font-medium",
          children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Tag, {
            className: "w-4 h-4 mr-2"
          }), "Featured Article"]
        }), (0, jsx_runtime_1.jsx)(link_1.default, {
          href: `/blog/${featuredPost.slug}`,
          children: (0, jsx_runtime_1.jsx)("h2", {
            className: "text-3xl font-bold mb-4 text-gray-900 hover:text-orange-600 transition-colors cursor-pointer",
            children: featuredPost.title
          })
        }), (0, jsx_runtime_1.jsx)("p", {
          className: "text-lg text-gray-700 mb-6 leading-relaxed",
          children: featuredPost.excerpt
        }), (0, jsx_runtime_1.jsxs)("div", {
          className: "flex items-center justify-between",
          children: [(0, jsx_runtime_1.jsxs)("div", {
            className: "flex items-center text-sm text-gray-500",
            children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Calendar, {
              className: "w-4 h-4 mr-2"
            }), featuredPost.date, (0, jsx_runtime_1.jsx)(lucide_react_1.Clock, {
              className: "w-4 h-4 ml-4 mr-2"
            }), featuredPost.readTime,
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[18][0]++, featuredPost.author) &&
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[18][1]++, (0, jsx_runtime_1.jsxs)(jsx_runtime_1.Fragment, {
              children: [(0, jsx_runtime_1.jsx)("span", {
                className: "ml-4 mr-2",
                children: "\u2022"
              }), featuredPost.author]
            }))]
          }), (0, jsx_runtime_1.jsx)(link_1.default, {
            href: `/blog/${featuredPost.slug}`,
            children: (0, jsx_runtime_1.jsx)("span", {
              className: "text-orange-600 hover:text-orange-700 font-medium cursor-pointer",
              children: "Read Article \u2192"
            })
          })]
        })]
      })
    })), (0, jsx_runtime_1.jsxs)("div", {
      className: "mb-8 space-y-6",
      children: [
      /* istanbul ignore next */
      (cov_2ne6nouzzk().b[19][0]++, tags.length > 0) &&
      /* istanbul ignore next */
      (cov_2ne6nouzzk().b[19][1]++, (0, jsx_runtime_1.jsxs)("div", {
        children: [(0, jsx_runtime_1.jsxs)("div", {
          className: "flex items-center mb-4",
          children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Tag, {
