                cov_2ne6nouzzk().s[80]++;
                return setSelectedTimeFilter(option.key);
              },
              className: `px-4 py-2 rounded-full text-sm font-medium transition-colors ${selectedTimeFilter === option.key ?
              /* istanbul ignore next */
              (cov_2ne6nouzzk().b[22][0]++, 'bg-orange-500 text-white') :
              /* istanbul ignore next */
              (cov_2ne6nouzzk().b[22][1]++, 'bg-gray-100 text-gray-600 hover:bg-gray-200')}`,
              children: [option.label, (0, jsx_runtime_1.jsxs)("span", {
                className: "ml-1.5 text-xs opacity-75",
                children: ["(", option.count, ")"]
              })]
            }, option.key);
          })
        })]
      }), (
      /* istanbul ignore next */
      (cov_2ne6nouzzk().b[23][0]++, selectedTag) ||
      /* istanbul ignore next */
      (cov_2ne6nouzzk().b[23][1]++, selectedTimeFilter !== 'all')) &&
      /* istanbul ignore next */
      (cov_2ne6nouzzk().b[23][2]++, (0, jsx_runtime_1.jsxs)("div", {
        className: "flex items-center gap-4 p-4 bg-gray-50 rounded-lg",
        children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Filter, {
          className: "w-4 h-4 text-gray-500"
        }), (0, jsx_runtime_1.jsxs)("div", {
          className: "flex items-center gap-2 text-sm",
          children: [(0, jsx_runtime_1.jsx)("span", {
            className: "text-gray-600",
            children: "Showing:"
          }),
          /* istanbul ignore next */
          (cov_2ne6nouzzk().b[24][0]++, selectedTag) &&
          /* istanbul ignore next */
          (cov_2ne6nouzzk().b[24][1]++, (0, jsx_runtime_1.jsx)("span", {
            className: "px-2 py-1 bg-tiffany-100 text-tiffany-700 rounded text-xs font-medium",
            children: selectedTag
          })),
          /* istanbul ignore next */
          (cov_2ne6nouzzk().b[25][0]++, selectedTimeFilter !== 'all') &&
          /* istanbul ignore next */
          (cov_2ne6nouzzk().b[25][1]++, (0, jsx_runtime_1.jsx)("span", {
            className: "px-2 py-1 bg-orange-100 text-orange-700 rounded text-xs font-medium",
            children:
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[27][0]++, (_a = timeFilterOptions.find(opt => {
              /* istanbul ignore next */
              cov_2ne6nouzzk().f[24]++;
              cov_2ne6nouzzk().s[81]++;
              return opt.key === selectedTimeFilter;
            })) === null) ||
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[27][1]++, _a === void 0) ?
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[26][0]++, void 0) :
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[26][1]++, _a.label)
          })), (0, jsx_runtime_1.jsx)("span", {
            className: "text-gray-500",
            children: "\u2022"
          }), (0, jsx_runtime_1.jsxs)("span", {
            className: "text-gray-600 font-medium",
            children: [filteredPosts.length, " post", filteredPosts.length !== 1 ?
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[28][0]++, 's') :
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[28][1]++, '')]
          })]
        }), (0, jsx_runtime_1.jsx)("button", {
          onClick: () => {
            /* istanbul ignore next */
            cov_2ne6nouzzk().f[25]++;
            cov_2ne6nouzzk().s[82]++;
            setSelectedTag(null);
            /* istanbul ignore next */
            cov_2ne6nouzzk().s[83]++;
            setSelectedTimeFilter('all');
          },
          className: "ml-auto text-xs text-gray-500 hover:text-gray-700 underline",
          children: "Clear all filters"
        })]
      }))]
    }), postsToShow.length > 0 ?
    /* istanbul ignore next */
    (cov_2ne6nouzzk().b[29][0]++, (0, jsx_runtime_1.jsx)("div", {
      className: "grid md:grid-cols-2 lg:grid-cols-3 gap-8 mb-16",
      children: postsToShow.map(post => {
        /* istanbul ignore next */
        cov_2ne6nouzzk().f[26]++;
        cov_2ne6nouzzk().s[84]++;
        return (0, jsx_runtime_1.jsxs)(Card_1.default, {
          className: "p-6 hover:shadow-lg transition-shadow",
          children: [(0, jsx_runtime_1.jsxs)("div", {
            className: "flex items-center text-sm text-gray-500 mb-4",
            children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Calendar, {
              className: "w-4 h-4 mr-2"
            }), post.date, (0, jsx_runtime_1.jsx)(lucide_react_1.Clock, {
              className: "w-4 h-4 ml-4 mr-2"
            }), post.readTime]
          }), (0, jsx_runtime_1.jsx)(link_1.default, {
            href: `/blog/${post.slug}`,
            children: (0, jsx_runtime_1.jsx)("h2", {
              className: "text-xl font-semibold mb-2 hover:text-orange-600 transition-colors cursor-pointer",
              children: post.title
            })
          }), (0, jsx_runtime_1.jsx)("p", {
            className: "text-gray-600 mb-4",
            children: post.excerpt
          }), (0, jsx_runtime_1.jsx)("div", {
            className: "flex flex-wrap gap-2",
            children: post.tags.map(tag => {
              /* istanbul ignore next */
              cov_2ne6nouzzk().f[27]++;
              cov_2ne6nouzzk().s[85]++;
              return (0, jsx_runtime_1.jsxs)("span", {
                className: "inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-gray-100 text-gray-800",
                children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Tag, {
                  className: "w-3 h-3 mr-1"
                }), tag]
              }, tag);
            })
          }),
          /* istanbul ignore next */
          (cov_2ne6nouzzk().b[30][0]++, post.author) &&
          /* istanbul ignore next */
          (cov_2ne6nouzzk().b[30][1]++, (0, jsx_runtime_1.jsx)("div", {
            className: "mt-4 pt-4 border-t border-gray-100",
            children: (0, jsx_runtime_1.jsxs)("p", {
              className: "text-sm text-gray-500",
              children: ["By ", post.author]
            })
          }))]
        }, post.slug);
      })
    })) :
    /* istanbul ignore next */
    (cov_2ne6nouzzk().b[29][1]++, (0, jsx_runtime_1.jsxs)("div", {
      className: "text-center py-16",
      children: [(0, jsx_runtime_1.jsx)("h3", {
        className: "text-xl font-semibold text-gray-900 mb-4",
        children: "No posts found"
      }), (0, jsx_runtime_1.jsx)("p", {
        className: "text-gray-600 mb-8",
        children:
        /* istanbul ignore next */
        (cov_2ne6nouzzk().b[32][0]++, selectedTag) ||
        /* istanbul ignore next */
        (cov_2ne6nouzzk().b[32][1]++, selectedTimeFilter !== 'all') ?
        /* istanbul ignore next */
        (cov_2ne6nouzzk().b[31][0]++, 'Try adjusting your filters or clear them to see all posts.') :
        /* istanbul ignore next */
        (cov_2ne6nouzzk().b[31][1]++, 'We have more great content in development. Check back soon for our latest insights and updates.')
      }), (
      /* istanbul ignore next */
      (cov_2ne6nouzzk().b[33][0]++, selectedTag) ||
      /* istanbul ignore next */
      (cov_2ne6nouzzk().b[33][1]++, selectedTimeFilter !== 'all')) &&
      /* istanbul ignore next */
      (cov_2ne6nouzzk().b[33][2]++, (0, jsx_runtime_1.jsx)("button", {
        onClick: () => {
          /* istanbul ignore next */
          cov_2ne6nouzzk().f[28]++;
          cov_2ne6nouzzk().s[86]++;
          setSelectedTag(null);
          /* istanbul ignore next */
          cov_2ne6nouzzk().s[87]++;
          setSelectedTimeFilter('all');
        },
        className: "px-6 py-3 bg-tiffany-500 text-white rounded-lg hover:bg-tiffany-600 transition-colors",
        children: "View All Posts"
      }))]
    })), (0, jsx_runtime_1.jsx)("div", {
      className: "mt-16",
      children: (0, jsx_runtime_1.jsxs)(Card_1.default, {
        className: "p-8 text-center",
        children: [(0, jsx_runtime_1.jsx)("h2", {
          className: "text-2xl font-bold mb-4",
          children: "Stay Updated"
        }), (0, jsx_runtime_1.jsx)("p", {
