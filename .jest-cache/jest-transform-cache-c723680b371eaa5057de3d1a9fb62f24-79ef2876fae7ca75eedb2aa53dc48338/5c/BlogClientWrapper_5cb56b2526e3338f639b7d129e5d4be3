              /* istanbul ignore next */
              cov_2ne6nouzzk().f[19]++;
              cov_2ne6nouzzk().s[76]++;
              return setSelectedTag(null);
            },
            className: `px-4 py-2 rounded-full text-sm font-medium transition-colors ${!selectedTag ?
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[20][0]++, 'bg-tiffany-500 text-white') :
            /* istanbul ignore next */
            (cov_2ne6nouzzk().b[20][1]++, 'bg-gray-100 text-gray-600 hover:bg-gray-200')}`,
            children: "All Topics"
          }), tags.map(tag => {
            /* istanbul ignore next */
            cov_2ne6nouzzk().f[20]++;
            cov_2ne6nouzzk().s[77]++;
            return (0, jsx_runtime_1.jsx)("button", {
              onClick: () => {
                /* istanbul ignore next */
                cov_2ne6nouzzk().f[21]++;
                cov_2ne6nouzzk().s[78]++;
