// Parse QR code data and determine format
function parseQRData(data) {
  /* istanbul ignore next */
  cov_787atmjtn().f[24]++;
  const cleanData =
  /* istanbul ignore next */
  (cov_787atmjtn().s[149]++, data.trim());
  // Bitcoin address patterns
  /* istanbul ignore next */
  cov_787atmjtn().s[150]++;
  if (
  /* istanbul ignore next */
  (cov_787atmjtn().b[57][0]++, /^[13][a-km-zA-HJ-NP-Z1-9]{25,34}$/.test(cleanData)) ||
  /* istanbul ignore next */
  (cov_787atmjtn().b[57][1]++, /^bc1[a-z0-9]{39,59}$/.test(cleanData))) {
    /* istanbul ignore next */
    cov_787atmjtn().b[56][0]++;
    cov_787atmjtn().s[151]++;
    return {
      data: cleanData,
      format: 'bitcoin',
      confidence: 0.9
    };
  } else
  /* istanbul ignore next */
  {
    cov_787atmjtn().b[56][1]++;
  }
  // Lightning invoice
  cov_787atmjtn().s[152]++;
  if (/^ln[a-z0-9]+$/i.test(cleanData)) {
    /* istanbul ignore next */
    cov_787atmjtn().b[58][0]++;
    cov_787atmjtn().s[153]++;
    return {
      data: cleanData,
      format: 'lightning',
      confidence: 0.9
    };
  } else
  /* istanbul ignore next */
  {
    cov_787atmjtn().b[58][1]++;
  }
  // Bitcoin URI
  cov_787atmjtn().s[154]++;
  if (cleanData.startsWith('bitcoin:')) {
    /* istanbul ignore next */
    cov_787atmjtn().b[59][0]++;
    cov_787atmjtn().s[155]++;
    return {
      data: cleanData,
      format: 'bitcoin',
      confidence: 0.8
    };
  } else
  /* istanbul ignore next */
  {
    cov_787atmjtn().b[59][1]++;
  }
  // Lightning URI
  cov_787atmjtn().s[156]++;
  if (cleanData.startsWith('lightning:')) {
    /* istanbul ignore next */
    cov_787atmjtn().b[60][0]++;
    cov_787atmjtn().s[157]++;
    return {
      data: cleanData,
      format: 'lightning',
      confidence: 0.8
    };
  } else
  /* istanbul ignore next */
  {
    cov_787atmjtn().b[60][1]++;
  }
  // URL
  cov_787atmjtn().s[158]++;
  if (/^https?:\/\/.+/.test(cleanData)) {
    /* istanbul ignore next */
    cov_787atmjtn().b[61][0]++;
    cov_787atmjtn().s[159]++;
    return {
      data: cleanData,
      format: 'url',
      confidence: 0.7
    };
  } else
  /* istanbul ignore next */
  {
    cov_787atmjtn().b[61][1]++;
  }
  // Default to text
  cov_787atmjtn().s[160]++;
  return {
    data: cleanData,
    format: 'text',
    confidence: 0.5
  };
}
// Hook for QR scanner availability
function useQRScanner() {
  /* istanbul ignore next */
  cov_787atmjtn().f[25]++;
  const [isAvailable, setIsAvailable] =
  /* istanbul ignore next */
  (cov_787atmjtn().s[161]++, (0, react_1.useState)(false));
  /* istanbul ignore next */
  cov_787atmjtn().s[162]++;
  (0, react_1.useEffect)(() => {
    /* istanbul ignore next */
    cov_787atmjtn().f[26]++;
    cov_787atmjtn().s[163]++;
    const checkAvailability = () => {
      /* istanbul ignore next */
      cov_787atmjtn().f[27]++;
      const hasCamera =
      /* istanbul ignore next */
      (cov_787atmjtn().s[164]++, !!(
      /* istanbul ignore next */
      (cov_787atmjtn().b[62][0]++, navigator.mediaDevices) &&
      /* istanbul ignore next */
      (cov_787atmjtn().b[62][1]++, navigator.mediaDevices.getUserMedia)));
      const hasCanvas =
      /* istanbul ignore next */
      (cov_787atmjtn().s[165]++, !!document.createElement('canvas').getContext);
      /* istanbul ignore next */
      cov_787atmjtn().s[166]++;
      setIsAvailable(
      /* istanbul ignore next */
      (cov_787atmjtn().b[63][0]++, hasCamera) &&
      /* istanbul ignore next */
      (cov_787atmjtn().b[63][1]++, hasCanvas));
    };
    /* istanbul ignore next */
    cov_787atmjtn().s[167]++;
    checkAvailability();
  }, []);
  /* istanbul ignore next */
  cov_787atmjtn().s[168]++;
  return isAvailable;
}
