          className: "h-5 w-5"
        }), (0, jsx_runtime_1.jsx)("span", {
          children: "Bitcoin"
        })]
      }),
      /* istanbul ignore next */
      (cov_253klj2ebg().b[1][0]++, lightningAddress) &&
      /* istanbul ignore next */
      (cov_253klj2ebg().b[1][1]++, (0, jsx_runtime_1.jsxs)("button", {
        onClick: () => {
          /* istanbul ignore next */
          cov_253klj2ebg().f[4]++;
          cov_253klj2ebg().s[16]++;
          return setActiveTab('lightning');
        },
        className: `flex items-center space-x-2 px-4 py-2 rounded-lg transition-colors ${activeTab === 'lightning' ?
        /* istanbul ignore next */
        (cov_253klj2ebg().b[2][0]++, 'bg-orange-100 text-orange-600') :
        /* istanbul ignore next */
        (cov_253klj2ebg().b[2][1]++, 'text-gray-600 hover:bg-gray-100')}`,
        children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Zap, {
          className: "h-5 w-5"
        }), (0, jsx_runtime_1.jsx)("span", {
          children: "Lightning"
        })]
      }))]
    }),
    /* istanbul ignore next */
    (cov_253klj2ebg().b[3][0]++, activeTab === 'bitcoin') &&
    /* istanbul ignore next */
    (cov_253klj2ebg().b[3][1]++, (0, jsx_runtime_1.jsxs)("div", {
      className: "space-y-6",
      children: [(0, jsx_runtime_1.jsxs)("div", {
        className: "text-center",
        children: [(0, jsx_runtime_1.jsx)("p", {
          className: "text-sm text-gray-600 mb-4",
          children: "Scan this QR code with your Bitcoin wallet to send funding"
        }), (0, jsx_runtime_1.jsx)("div", {
          className: "flex justify-center p-4 bg-white rounded-lg border border-gray-200",
          children: (0, jsx_runtime_1.jsx)(qrcode_react_1.QRCodeSVG, {
            value: `bitcoin:${walletAddress}`,
            size: 200,
            className: "rounded-lg"
          })
        })]
      }), (0, jsx_runtime_1.jsxs)("div", {
        children: [(0, jsx_runtime_1.jsx)("p", {
          className: "text-sm text-gray-600 mb-2",
          children: "Or copy the address below"
        }), (0, jsx_runtime_1.jsxs)("div", {
          className: "flex items-center space-x-2",
          children: [(0, jsx_runtime_1.jsx)("input", {
            type: "text",
            value: walletAddress,
            readOnly: true,
            className: "flex-1 p-2 text-sm border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-orange-500"
          }), (0, jsx_runtime_1.jsx)("button", {
            onClick: handleCopy,
            className: "p-2 text-gray-600 hover:text-gray-900 transition-colors",
            "aria-label": "Copy address",
            children: copied ?
            /* istanbul ignore next */
            (cov_253klj2ebg().b[4][0]++, (0, jsx_runtime_1.jsx)(lucide_react_1.Check, {
              className: "h-5 w-5 text-green-500"
            })) :
            /* istanbul ignore next */
            (cov_253klj2ebg().b[4][1]++, (0, jsx_runtime_1.jsx)(lucide_react_1.Copy, {
              className: "h-5 w-5"
            }))
          })]
        })]
      })]
    })),
    /* istanbul ignore next */
    (cov_253klj2ebg().b[5][0]++, activeTab === 'lightning') &&
    /* istanbul ignore next */
    (cov_253klj2ebg().b[5][1]++, lightningAddress) &&
    /* istanbul ignore next */
    (cov_253klj2ebg().b[5][2]++, (0, jsx_runtime_1.jsxs)("div", {
      className: "space-y-6",
      children: [(0, jsx_runtime_1.jsxs)("div", {
        className: "text-center",
        children: [(0, jsx_runtime_1.jsx)("p", {
          className: "text-sm text-gray-600 mb-4",
          children: "Scan this QR code with your Lightning wallet to send funding"
        }), (0, jsx_runtime_1.jsx)("div", {
          className: "flex justify-center p-4 bg-white rounded-lg border border-gray-200",
          children: (0, jsx_runtime_1.jsx)(qrcode_react_1.QRCodeSVG, {
            value: `lightning:${lightningAddress}`,
            size: 200,
            className: "rounded-lg"
          })
        })]
      }), (0, jsx_runtime_1.jsxs)("div", {
        children: [(0, jsx_runtime_1.jsx)("p", {
          className: "text-sm text-gray-600 mb-2",
          children: "Or copy the address below"
        }), (0, jsx_runtime_1.jsxs)("div", {
          className: "flex items-center space-x-2",
          children: [(0, jsx_runtime_1.jsx)("input", {
            type: "text",
            value: lightningAddress,
            readOnly: true,
            className: "flex-1 p-2 text-sm border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-orange-500"
          }), (0, jsx_runtime_1.jsx)("button", {
            onClick: () => {
              /* istanbul ignore next */
              cov_253klj2ebg().f[5]++;
              cov_253klj2ebg().s[17]++;
              return navigator.clipboard.writeText(lightningAddress);
            },
            className: "p-2 text-gray-600 hover:text-gray-900 transition-colors",
            "aria-label": "Copy address",
            children: (0, jsx_runtime_1.jsx)(lucide_react_1.Copy, {
              className: "h-5 w-5"
            })
          })]
        })]
      })]
    }))]
  });
}
