                children: "Lightning Benefits"
              }), (0, jsx_runtime_1.jsxs)("ul", {
                className: "text-sm text-blue-700 space-y-1",
                children: [(0, jsx_runtime_1.jsx)("li", {
                  children: "\u2022 Instant payments (usually under 3 seconds)"
                }), (0, jsx_runtime_1.jsx)("li", {
                  children: "\u2022 Extremely low fees (typically <1 sat)"
                }), (0, jsx_runtime_1.jsx)("li", {
                  children: "\u2022 Perfect for small amounts and tips"
                })]
              })]
            })]
          })
        }), (0, jsx_runtime_1.jsx)(Button_1.default, {
          onClick: generateInvoice,
          disabled:
          /* istanbul ignore next */
          (cov_2lboad4czs().b[15][0]++, isGenerating) ||
          /* istanbul ignore next */
          (cov_2lboad4czs().b[15][1]++, !amount),
          className: "w-full",
          children: isGenerating ?
          /* istanbul ignore next */
          (cov_2lboad4czs().b[16][0]++, (0, jsx_runtime_1.jsxs)(jsx_runtime_1.Fragment, {
            children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Loader2, {
              className: "w-4 h-4 mr-2 animate-spin"
            }), "Generating Invoice..."]
          })) :
          /* istanbul ignore next */
          (cov_2lboad4czs().b[16][1]++, 'Generate Lightning Invoice')
        })]
      })) :
      /* istanbul ignore next */
      (// Invoice Display
      cov_2lboad4czs().b[14][1]++, (0, jsx_runtime_1.jsxs)("div", {
        className: "space-y-4",
        children: [(0, jsx_runtime_1.jsxs)("div", {
          className: "text-center",
          children: [(0, jsx_runtime_1.jsx)(CurrencyDisplay_1.CurrencyDisplay, {
            amount: invoice.amount,
            currency: "SATS",
            className: "text-xl font-semibold"
          }),
          /* istanbul ignore next */
          (cov_2lboad4czs().b[17][0]++, invoice.description) &&
          /* istanbul ignore next */
          (cov_2lboad4czs().b[17][1]++, (0, jsx_runtime_1.jsx)("p", {
            className: "text-sm text-gray-600 mt-1",
            children: invoice.description
          }))]
        }),
        /* istanbul ignore next */
        (cov_2lboad4czs().b[18][0]++, paymentStatus !== 'expired') &&
        /* istanbul ignore next */
        (cov_2lboad4czs().b[18][1]++, (0, jsx_runtime_1.jsx)("div", {
          className: "flex justify-center p-4 bg-white border border-gray-200 rounded-lg",
          children: (0, jsx_runtime_1.jsx)(qrcode_react_1.QRCodeSVG, {
            value: invoice.bolt11.toUpperCase(),
            size: 200,
            level: "M",
            includeMargin: true
          })
        })), (0, jsx_runtime_1.jsxs)("div", {
          children: [(0, jsx_runtime_1.jsx)("label", {
            className: "block text-sm font-medium text-gray-700 mb-2",
            children: "Lightning Invoice"
          }), (0, jsx_runtime_1.jsxs)("div", {
            className: "flex gap-2",
            children: [(0, jsx_runtime_1.jsx)("div", {
              className: "flex-1 p-3 bg-gray-50 rounded-lg border border-gray-200",
              children: (0, jsx_runtime_1.jsx)("code", {
                className: "text-xs text-gray-600 break-all font-mono",
                children: invoice.bolt11
              })
            }), (0, jsx_runtime_1.jsx)(Button_1.default, {
              onClick: copyInvoice,
              variant: "outline",
              size: "sm",
              className: copied ?
              /* istanbul ignore next */
              (cov_2lboad4czs().b[19][0]++, 'bg-green-50 text-green-700 border-green-200') :
              /* istanbul ignore next */
              (cov_2lboad4czs().b[19][1]++, ''),
              children: copied ?
              /* istanbul ignore next */
              (cov_2lboad4czs().b[20][0]++, (0, jsx_runtime_1.jsx)(lucide_react_1.Check, {
                className: "w-4 h-4"
              })) :
              /* istanbul ignore next */
              (cov_2lboad4czs().b[20][1]++, (0, jsx_runtime_1.jsx)(lucide_react_1.Copy, {
                className: "w-4 h-4"
              }))
            })]
          })]
        }), (0, jsx_runtime_1.jsx)("div", {
          className: "flex gap-2",
          children: (0, jsx_runtime_1.jsxs)(Button_1.default, {
            onClick: () => {
              /* istanbul ignore next */
              cov_2lboad4czs().f[13]++;
              cov_2lboad4czs().s[76]++;
              return window.open(`lightning:${invoice.bolt11}`, '_blank');
            },
            className: "flex-1",
            disabled: paymentStatus === 'expired',
            children: [(0, jsx_runtime_1.jsx)(lucide_react_1.ExternalLink, {
              className: "w-4 h-4 mr-2"
            }), "Open in Wallet"]
          })
        }), (
        /* istanbul ignore next */
        (cov_2lboad4czs().b[21][0]++, paymentStatus === 'expired') ||
        /* istanbul ignore next */
        (cov_2lboad4czs().b[21][1]++, paymentStatus === 'failed')) &&
        /* istanbul ignore next */
        (cov_2lboad4czs().b[21][2]++, (0, jsx_runtime_1.jsx)(Button_1.default, {
          onClick: resetPayment,
          variant: "outline",
          className: "w-full",
          children: "Generate New Invoice"
        })), (0, jsx_runtime_1.jsxs)("div", {
          className: "bg-yellow-50 border border-yellow-200 rounded-lg p-3",
          children: [(0, jsx_runtime_1.jsx)("h4", {
            className: "font-medium text-yellow-900 mb-2",
            children: "How to Pay:"
          }), (0, jsx_runtime_1.jsxs)("ol", {
            className: "text-sm text-yellow-800 space-y-1",
            children: [(0, jsx_runtime_1.jsx)("li", {
              children: "1. Open your Lightning wallet app"
            }), (0, jsx_runtime_1.jsx)("li", {
              children: "2. Scan the QR code or paste the invoice"
            }), (0, jsx_runtime_1.jsx)("li", {
              children: "3. Confirm the payment in your wallet"
            }), (0, jsx_runtime_1.jsx)("li", {
              children: "4. Payment will be confirmed instantly"
            })]
          })]
        }),
        /* istanbul ignore next */
        (cov_2lboad4czs().b[22][0]++, timeLeft !== null) &&
        /* istanbul ignore next */
        (cov_2lboad4czs().b[22][1]++, timeLeft > 0) &&
        /* istanbul ignore next */
        (cov_2lboad4czs().b[22][2]++, (0, jsx_runtime_1.jsxs)("div", {
          className: "text-center text-sm text-gray-500",
          children: ["Invoice expires in ", formatTime(timeLeft)]
        }))]
      }))
    })]
  });
}
