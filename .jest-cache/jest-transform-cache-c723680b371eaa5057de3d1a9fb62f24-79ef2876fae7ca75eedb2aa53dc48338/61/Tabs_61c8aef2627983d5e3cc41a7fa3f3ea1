    className: (0, utils_1.cn)('inline-flex h-10 items-center justify-center rounded-md bg-muted p-1 text-muted-foreground', className),
    children: children
  });
}
function TabsTrigger({
  value,
  children,
  className
}) {
  /* istanbul ignore next */
  cov_boxbeujfs().f[4]++;
  const {
    value: selectedValue,
    onValueChange
  } =
  /* istanbul ignore next */
  (cov_boxbeujfs().s[22]++, useTabs());
  const isSelected =
  /* istanbul ignore next */
  (cov_boxbeujfs().s[23]++, selectedValue === value);
  /* istanbul ignore next */
  cov_boxbeujfs().s[24]++;
  return (0, jsx_runtime_1.jsx)("button", {
    onClick: () => {
      /* istanbul ignore next */
      cov_boxbeujfs().f[5]++;
      cov_boxbeujfs().s[25]++;
      return onValueChange(value);
    },
    className: (0, utils_1.cn)('inline-flex items-center justify-center whitespace-nowrap rounded-sm px-3 py-1.5 text-sm font-medium ring-offset-background transition-all focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:pointer-events-none disabled:opacity-50', isSelected ?
    /* istanbul ignore next */
    (cov_boxbeujfs().b[4][0]++, 'bg-background text-foreground shadow-sm') :
    /* istanbul ignore next */
    (cov_boxbeujfs().b[4][1]++, 'hover:bg-muted hover:text-foreground'), className),
    children: children
  });
}
function TabsContent({
  value,
  children,
  className
}) {
  /* istanbul ignore next */
  cov_boxbeujfs().f[6]++;
  const {
    value: selectedValue
  } =
  /* istanbul ignore next */
  (cov_boxbeujfs().s[26]++, useTabs());
  /* istanbul ignore next */
  cov_boxbeujfs().s[27]++;
  if (selectedValue !== value) {
    /* istanbul ignore next */
    cov_boxbeujfs().b[5][0]++;
    cov_boxbeujfs().s[28]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_boxbeujfs().b[5][1]++;
  }
  cov_boxbeujfs().s[29]++;
  return (0, jsx_runtime_1.jsx)("div", {
    className: (0, utils_1.cn)('mt-2 focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2', className),
    children: children
  });
}
