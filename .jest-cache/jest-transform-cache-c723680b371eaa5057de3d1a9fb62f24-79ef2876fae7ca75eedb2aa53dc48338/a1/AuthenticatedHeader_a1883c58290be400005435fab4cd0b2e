          (cov_13sgyd9gpn().b[4][1]++, (0, jsx_runtime_1.jsx)(DefaultAvatar_1.default, {
            size: 24,
            className: "rounded-full"
          })), (0, jsx_runtime_1.jsx)("span", {
            className: "text-sm font-medium text-gray-700 hidden lg:block max-w-20 truncate",
            children:
            /* istanbul ignore next */
            (cov_13sgyd9gpn().b[6][0]++, profile.display_name) ||
            /* istanbul ignore next */
            (cov_13sgyd9gpn().b[6][1]++, profile.username)
          })]
        }))]
      })]
    }), (0, jsx_runtime_1.jsx)(MobileSearchModal_1.default, {
      isOpen: showMobileSearch,
      onClose: () => {
        /* istanbul ignore next */
        cov_13sgyd9gpn().f[3]++;
        cov_13sgyd9gpn().s[18]++;
        return setShowMobileSearch(false);
      }
    })]
  });
}
